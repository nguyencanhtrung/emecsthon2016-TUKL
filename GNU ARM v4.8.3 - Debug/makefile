# Tool invocations
emecsthon16.axf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: GNU ARM C Linker'
	arm-none-eabi-gcc -g -gdwarf-2 -mcpu=cortex-m3 -mthumb -T "emecsthon16.ld" -Xlinker --gc-sections -Xlinker -Map="emecsthon16.map" --specs=nano.specs -o emecsthon16.axf "./src/gpio.o" "./src/main.o" "./src/pwm.o" "./src/uart.o" "./emlib/em_assert.o" "./emlib/em_cmu.o" "./emlib/em_ebi.o" "./emlib/em_emu.o" "./emlib/em_gpio.o" "./emlib/em_system.o" "./emlib/em_timer.o" "./emlib/em_usart.o" "./CMSIS/efm32gg/startup_efm32gg.o" "./CMSIS/efm32gg/system_efm32gg.o" "./BSP/bsp_bcc.o" "./BSP/bsp_stk.o" "./BSP/bsp_stk_leds.o" "./BSP/bsp_trace.o" -Wl,--start-group -lgcc -lc -lnosys -Wl,--end-group
	@echo 'Finished building target: $@'
	@echo ' '

	@echo 'Building hex file: emecsthon16.hex'
	arm-none-eabi-objcopy -O ihex "emecsthon16.axf" "emecsthon16.hex"
	@echo ' '

	@echo 'Building bin file: emecsthon16.bin'
	arm-none-eabi-objcopy -O binary "emecsthon16.axf" "emecsthon16.bin"
	@echo ' '

	@echo 'Running size tool'
	arm-none-eabi-size "emecsthon16.axf"
	@echo ' '

